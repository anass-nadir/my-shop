{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "ESNext",
    "lib": ["ES2019", "DOM", "DOM.Iterable", "ESNext"],
    "outDir": "./build",
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "rootDir": "./src",
    "allowJs": true
  },
  "include": ["./src"]
}
